    "import random\n",
    "import os\n",
    "import pygame\n",
    "import numpy as np\n",
    "import csv\n",
    "(width, height) = (600,600)\n",
    "\n",
    "class Chicken:\n",
    "    #Creates the class of Chicken, to be reused\n",
    "    def __init__(self):\n",
    "        self.size = 15\n",
    "        self.x = int((270))\n",
    "        self.y = int((510))\n",
    "        \n",
    "class Present:\n",
    "    #Creates the class of Present, to be reused\n",
    "    def __init__(self,chickenX,chickenY,upper,lower):\n",
    "        self.size = 40\n",
    "        #Random X, that is not the same place as the chicken\n",
    "        notX = (chickenX + 30) / 60\n",
    "        randX = notX\n",
    "        while randX == notX:\n",
    "            randX = random.randint(upper,lower)\n",
    "        #Random Y, that is not the same place as the chicken\n",
    "        notY = (chickenY + 30) / 60\n",
    "        randY = notY\n",
    "        while randY == notY:\n",
    "            randY = random.randint(upper,lower)\n",
    "        \n",
    "        self.x = ((randX-1)*60) + 10\n",
    "        self.y = ((randY-1)*60) + 10        \n",
    "\n",
    "class Car():\n",
    "    #Creates the class of Car, to be reused\n",
    "    def __init__(self,carLarge):\n",
    "        if random.randint(1,2) == 1:\n",
    "            self.direction = \"right\"\n",
    "        else:\n",
    "            self.direction = \"left\"\n",
    "            self.x = 550\n",
    "        randY = random.randint(0,10) \n",
    "        self.y = ((randY-1)*60) + 10  \n",
    "        \n",
    "        rand = random.uniform(0,1)\n",
    "        self.large = carLarge\n",
    "        if rand < self.large:\n",
    "            self.len = 3\n",
    "        elif rand < ((1 - self.large)/2):\n",
    "            self.len = 2\n",
    "        else:\n",
    "            self.len = 1\n",
    "        \n",
    "        if self.len == 1:\n",
    "            self.size = 40\n",
    "            if self.direction == \"right\":\n",
    "                self.x = 10\n",
    "        elif self.len == 2:\n",
    "            self.size = 100\n",
    "            if self.direction == \"right\":\n",
    "                self.x = -50\n",
    "        elif self.len == 3:\n",
    "            self.size = 160\n",
    "            if self.direction == \"right\":\n",
    "                self.x = -110\n",
    "\n",
    "def analyse(chX,chY,pDict,cDict):\n",
    "    #Function to analyse the surroundings of the chicken and return sTable, which shows '0' if empty, '1' if Present or '2' if car\n",
    "    cXno = (chX + 30) / 60\n",
    "    cYno = (chY + 30) / 60\n",
    "    \n",
    "    xSize = 3\n",
    "    ySize = 5\n",
    "    sTable = np.zeros((xSize, ySize))\n",
    "    \n",
    "    \n",
    "    for x in range(0,5):\n",
    "        xA =  x - 2\n",
    "        for y in range(0,3):\n",
    "            yA =  y - 1\n",
    "            \n",
    "            pX = (chX + (xA*60)) - 20\n",
    "            pY = (chY + (yA*60)) - 20\n",
    "        \n",
    "            \n",
    "            for p in pDict:\n",
    "                if pDict[p][\"x\"] == pX and pDict[p][\"y\"] == pY:\n",
    "                    sTable[y][x] = 1\n",
    "                    break\n",
    "                    \n",
    "            for c in cDict:\n",
    "                if cDict[c][\"x\"] == pX and cDict[c][\"y\"] == pY:\n",
    "                    sTable[y][x] = 2\n",
    "                    if cDict[c][\"direction\"] == \"right\":\n",
    "                        if cDict[c][\"size\"] == 100 and (x-1) > -1:\n",
    "                            sTable[y][x-1] = 2\n",
    "                        if cDict[c][\"size\"] == 160 and (x-2) > -1:\n",
    "                            sTable[y][x-2] = 2\n",
    "                    elif cDict[c][\"direction\"] == \"left\":\n",
    "                        if cDict[c][\"size\"] == 100 and (x+1) < 5:\n",
    "                            sTable[y][x+1] = 2\n",
    "                        if cDict[c][\"size\"] == 160 and (x+2) < 5:\n",
    "                            sTable[y][x+2] = 2\n",
    "        \n",
    "                        \n",
    "    return sTable\n",
    "\n",
    "def sTableToBinary(sTable):\n",
    "    #Turns the sTable to binary list of status of positions around the chicken\n",
    "    sBin1 =  str(int(sTable[0][1])) + str(int(sTable[0][2])) + str(int(sTable[0][3])) \n",
    "    sBin2 = str(int(sTable[1][0])) + str(int(sTable[1][1]))  + str(int(sTable[1][3])) + str(int(sTable[1][4]))\n",
    "    sBin3 =  str(int(sTable[2][1])) + str(int(sTable[2][2])) + str(int(sTable[2][3])) \n",
    "    sBin = sBin1 + sBin2 + sBin3\n",
    "    return sBin\n",
    "\n",
    "def binaryTosTable(sBin):\n",
    "    #Turns the Binary list of surrounding positions into the sTable\n",
    "    sTable = np.zeros((3,5))\n",
    "    sTable[0][0],sTable[0][1],sTable[0][2],sTable[0][3],sTable[0][4] = 0,int(sBin[0]),int(sBin[1]),int(sBin[2]),0\n",
    "    sTable[1][0],sTable[1][1],sTable[1][2],sTable[1][3],sTable[1][4] = int(sBin[3]),int(sBin[4]),0,int(sBin[5]),int(sBin[6])\n",
    "    sTable[2][0],sTable[2][1],sTable[2][2],sTable[2][3],sTable[2][4] = 0,int(sBin[7]),int(sBin[8]),int(sBin[9]),0\n",
    "    return sTable\n",
    "\n",
    "def createqDict():\n",
    "    #Creates an initial q-table Dictionary of all states possible, with rewards for each action\n",
    "    qDict = {\"0000000000\": {\"u\":0,\"r\":0,\"d\":0,\"l\":0}}\n",
    "    for a in range(3):\n",
    "        for b in range(3):\n",
    "            for c in range(3):\n",
    "                for d in range(3):\n",
    "                    for e in range(3):\n",
    "                        for f in range(3):\n",
    "                            for g in range(3):\n",
    "                                for h in range(3):\n",
    "                                    for i in range(3):\n",
    "                                        for j in range(3):\n",
    "                                            key = str(a)+str(b)+str(c)+str(d)+str(e)+str(f)+str(g)+str(h)+str(i)+str(j)\n",
    "                                            qDict[key] = {\"u\":0,\"r\":0,\"d\":0,\"l\":0}\n",
    "    return qDict\n",
    "\n",
    "\n",
    "def decideMove(qDict,sTable,epsilon,cX,cY):\n",
    "    #Analyses the q-table Dictionary to understand the best move for the current state\n",
    "    sBin = sTableToBinary(sTable)\n",
    "    rand = random.uniform(0,1)\n",
    "    if rand < epsilon or sBin == \"0000000000\":\n",
    "        #Choose randomly for epsilon amount of time\n",
    "        finish = False\n",
    "        while finish == False:\n",
    "            moveRand = random.randint(1,4)\n",
    "            if moveRand == 1:\n",
    "                move = \"u\"\n",
    "                if cY > 60:\n",
    "                    finish = True\n",
    "            elif moveRand == 2:\n",
    "                move = \"r\"\n",
    "                if cX < 540:\n",
    "                    finish = True\n",
    "            elif moveRand == 3:\n",
    "                move = \"d\"\n",
    "                if cY < 540:\n",
    "                    finish = True\n",
    "            elif moveRand == 4:\n",
    "                move = \"l\"\n",
    "                if cX > 60:\n",
    "                    finish = True\n",
    "    else:\n",
    "        qDictTemp = qDict\n",
    "        #Punish a movement into the wall\n",
    "        if cY < 60:\n",
    "            qDictTemp[sBin][\"u\"] = -1000\n",
    "        if cY > 540:\n",
    "            qDictTemp[sBin][\"d\"] = -1000\n",
    "        if cX < 60:\n",
    "            qDictTemp[sBin][\"l\"] = -1000\n",
    "        if cX > 540:\n",
    "            qDictTemp[sBin][\"r\"] = -1000\n",
    "        #Choose the action with highest reward from the qtable\n",
    "        mv = max(qDictTemp[sBin].values())\n",
    "        move = random.choice([k for (k, v) in qDictTemp[sBin].items() if v == mv])\n",
    "    \n",
    "    return move\n",
    "\n",
    "\n",
    "def reward(qDict,key,move,newScore,oldScore):\n",
    "    qDict[key][move] = oldScore - newScore \n",
    "    return qDict\n",
    "\n",
    "def writeCSV(qDict):\n",
    "    #Function to write the Q-table to excel form\n",
    "    filename = 'C:\\\\Users\\\\b.saunders\\\\Documents\\\\Python\\\\Projects\\\\Chicken Cross\\\\ChickenCross.csv'\n",
    "    with open(filename, 'w') as f: \n",
    "        w = csv.writer(f)\n",
    "        w.writerows(qDict.items())\n",
    "    \n",
    "    return\n",
    "\n",
    "def readCSV():\n",
    "    #Function to retrieve the Q-table from the excel into a dictionary\n",
    "    filename = 'C:\\\\Users\\\\b.saunders\\\\Documents\\\\Python\\\\Projects\\\\Chicken Cross\\\\ChickenCross.csv'\n",
    "    with open(filename) as f:\n",
    "        d = dict(filter(None, csv.reader(f)))\n",
    "    for key in d:\n",
    "        d[key] = eval(d[key])\n",
    "    qDict = d\n",
    "\n",
    "    return qDict\n",
    "\n",
    "\n",
    "def game(qDict,epsilon,episode):\n",
    "    #Central function that create the screen display, systematically creates cars and presents, and determines the movements of the chicken\n",
    "    \n",
    "    pygame.init()\n",
    "    pygame.font.init()\n",
    "\n",
    "    black = (0,0,0)\n",
    "    white = (255,255,255)\n",
    "    red = (255,0,0)\n",
    "    grey = (105,105,105)\n",
    "    orange = (255,215,0)\n",
    "    blue = (0,0,255)\n",
    "    \n",
    "    count = 0\n",
    "    chickenWait = False\n",
    "    pCount = 1\n",
    "    pFrequency = 100\n",
    "    cCount = 1\n",
    "    cFrequency = 100\n",
    "    cSpeed = 30\n",
    "    moveFrequency = 50\n",
    "    carLarge = 0.2\n",
    "    pScore = 0\n",
    "    AIfrequency = 1\n",
    "    \n",
    "    LR = 0.5\n",
    "    DF = 0.5\n",
    "    \n",
    "    quicker = False\n",
    "    finish = False\n",
    "    \n",
    "    #Initial parameters\n",
    "    (width, height) = (600,600)\n",
    "\n",
    "    screen = pygame.display.set_mode((width,height))\n",
    "    pygame.display.set_caption('Chicken Cross')\n",
    "    clock = pygame.time.Clock()  \n",
    "    \n",
    "    chicken = Chicken()\n",
    "    \n",
    "    present = Present(chicken.x,chicken.y,1,9)\n",
    "    pName = \"p\" + str(pCount)\n",
    "    pDict = {pName : {'x':present.x,'y':present.y} }\n",
    "    \n",
    "    for i in range(2):\n",
    "        present = Present(chicken.x,chicken.y,1,9)\n",
    "        pCount += 1 \n",
    "        pName = \"p\" + str(pCount)\n",
    "        pDict[pName] = {'x':present.x,'y':present.y} \n",
    "    \n",
    "    for i in range(2):\n",
    "        present = Present(chicken.x,chicken.y,9,10)\n",
    "        pCount += 1\n",
    "        pName = \"p\" + str(pCount)\n",
    "        pDict[pName] = {'x':present.x,'y':present.y}     \n",
    "    \n",
    "    car = Car(carLarge)\n",
    "    cName = \"c\" + str(cCount)\n",
    "    cDict = {cName : {'x':car.x,'y':car.y,'size':car.size,'direction':car.direction,'new':'yes'} }    \n",
    "    \n",
    "\n",
    "    play = True\n",
    "    \n",
    "    while play:    \n",
    "        \n",
    "        count += 1\n",
    "    \n",
    "       \n",
    "\n",
    "#Objects                    \n",
    "    #Presents\n",
    "        if count % pFrequency == 0:\n",
    "            for i in range(random.randint(0,3)):\n",
    "                present = Present(chicken.x,chicken.y,1,10)\n",
    "                pCount += 1 \n",
    "                pName = \"p\" + str(pCount)\n",
    "                pDict[pName] = {'x':present.x,'y':present.y} \n",
    "            \n",
    "    #Cars\n",
    "        if count % cFrequency == 0:\n",
    "            for i in range(random.randint(0,3)):\n",
    "                car = Car(carLarge)\n",
    "                cCount += 1 \n",
    "                cName = \"c\" + str(cCount)\n",
    "                cDict[cName] = {'x':car.x,'y':car.y,'size':car.size,'direction':car.direction,'new':'yes'} \n",
    "\n",
    "            \n",
    "            \n",
    "#Movement\n",
    "    #Cars\n",
    "        if count % cSpeed == 0:\n",
    "            for c in cDict.copy():\n",
    "                if cDict[c][\"new\"] == 'yes':\n",
    "                    cDict[c][\"new\"] = 'no'\n",
    "                else:\n",
    "                    if cDict[c][\"direction\"] == 'left':\n",
    "                        cDict[c][\"x\"] -= 60\n",
    "                        if cDict[c][\"x\"] < 0:\n",
    "                            del cDict[c]\n",
    "                    elif cDict[c][\"direction\"] == 'right':\n",
    "                        cDict[c][\"x\"] += 60\n",
    "                        if cDict[c][\"x\"] > 600:\n",
    "                            del cDict[c]\n",
    "                \n",
    "\n",
    "                    \n",
    "                    \n",
    "    #Screen upwards            \n",
    "        if count % moveFrequency == 0 and chicken.y < 540:\n",
    "            for p in pDict:\n",
    "                pDict[p][\"y\"] += 60\n",
    "            for c in cDict:\n",
    "                cDict[c][\"y\"] += 60\n",
    "            chicken.y += 60\n",
    "            \n",
    "            for i in range(random.randint(0,4)):\n",
    "                present = Present(chicken.x,chicken.y,0,5)\n",
    "                pCount += 1 \n",
    "                pName = \"p\" + str(pCount)\n",
    "                pDict[pName] = {'x':present.x,'y':present.y} \n",
    "                \n",
    "                \n",
    "    #AI element - Movement depending on the Q-table or randomness (depending on epsilon)\n",
    "        AIfrequency = 10\n",
    "        \n",
    "        if count % AIfrequency == 0:\n",
    "            oldScore = pScore\n",
    "            \n",
    "            oldSTable = analyse(chicken.x,chicken.y,pDict,cDict)\n",
    "            \n",
    "            move = decideMove(qDict,oldSTable,epsilon,chicken.x,chicken.y)\n",
    "            if move == \"u\":\n",
    "                if chicken.y > 60:\n",
    "                    chicken.y -= 60\n",
    "            if move == \"d\":\n",
    "                if chicken.y < 540:\n",
    "                    chicken.y += 60\n",
    "            if move == \"l\":\n",
    "                if chicken.x > 60:\n",
    "                    chicken.x -= 60\n",
    "            if move == \"r\":\n",
    "                if chicken.x < 540:\n",
    "                    chicken.x += 60\n",
    "                    \n",
    "            newSTable = analyse(chicken.x,chicken.y,pDict,cDict)\n",
    "                    \n",
    "            \n",
    "#Collisions\n",
    "    #Presents\n",
    "        chX = (chicken.x + 30) / 60 \n",
    "        pX = ((chX-1)*60) + 10\n",
    "        \n",
    "        chY = (chicken.y + 30) / 60\n",
    "        pY = ((chY-1)*60) + 10\n",
    "        \n",
    "        for p in pDict:\n",
    "            if pX == pDict[p][\"x\"] and pY == pDict[p][\"y\"]:\n",
    "                pScore += 1\n",
    "                del pDict[p]\n",
    "                break\n",
    "    \n",
    "    #Cars\n",
    "        carX = ((chX-1)*60) + 10\n",
    "        \n",
    "        carY = ((chY-1)*60) + 10 \n",
    "        \n",
    "        #Show the cars on the board - Larger cars is \"Size\" == 100 or 160\n",
    "        for c in cDict:\n",
    "            if cDict[c][\"size\"] == 40:\n",
    "                if carX == cDict[c][\"x\"] and carY == cDict[c][\"y\"]:\n",
    "                    play = False\n",
    "                    break    \n",
    "            if cDict[c][\"size\"] == 100:\n",
    "                if carX == cDict[c][\"x\"] and carY == cDict[c][\"y\"] or \\\n",
    "                carX == (cDict[c][\"x\"] + 60) and carY == cDict[c][\"y\"]:\n",
    "                    play = False\n",
    "                    break \n",
    "            if cDict[c][\"size\"] == 160:\n",
    "                if carX == cDict[c][\"x\"] and carY == cDict[c][\"y\"] or \\\n",
    "                carX == (cDict[c][\"x\"] + 60) and carY == cDict[c][\"y\"] or \\\n",
    "                carX == (cDict[c][\"x\"] + 120) and carY == cDict[c][\"y\"] :\n",
    "                    play = False\n",
    "                    break\n",
    "\n",
    "    \n",
    "    #AI Element - Reward scoring after decision is chosen and action played out\n",
    "        if count % AIfrequency == 0:\n",
    "            \n",
    "            newSBin = sTableToBinary(newSTable)\n",
    "            maximum = qDict[newSBin][max(qDict[newSBin], key=qDict[newSBin].get)]\n",
    "            \n",
    "            reward = 10*(pScore - oldScore) - 0.1\n",
    "            if play == False:\n",
    "                reward -= 100\n",
    "            \n",
    "            oldSBin = sTableToBinary(oldSTable)\n",
    "            \n",
    "            qDict[oldSBin][move] = (1-LR)*(qDict[oldSBin][move]) + LR*(reward + DF*(maximum))\n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    #Draw\n",
    "        \n",
    "        screen.fill(white)\n",
    "        #Gridlines\n",
    "        lines = 10\n",
    "        #Vertical\n",
    "        for i in range(1,lines):\n",
    "            pygame.draw.rect(screen, grey, ((i*width/lines),0,1,height))\n",
    "        #Horizontal\n",
    "        for i in range(1,lines):\n",
    "            pygame.draw.rect(screen, grey, (0,(i*height/lines),width,1))\n",
    "            \n",
    "        #Chicken\n",
    "        pygame.draw.circle(screen, red, (chicken.x,chicken.y), chicken.size)\n",
    "        \n",
    "        #Presents\n",
    "        for p in pDict:\n",
    "            pygame.draw.rect(screen, orange, (pDict[p][\"x\"],pDict[p][\"y\"],40,40))\n",
    "            \n",
    "        #Cars\n",
    "        for c in cDict:\n",
    "            pygame.draw.rect(screen, blue, (cDict[c][\"x\"],cDict[c][\"y\"],cDict[c][\"size\"],40))    \n",
    "\n",
    "\n",
    "            \n",
    "    \n",
    "        #Score\n",
    "        myFont = pygame.font.SysFont('Comic Sans MS', 20) \n",
    "        scoreText1 = \"Score: \" + str(pScore)\n",
    "        textSurface1 = myFont.render(scoreText1, False, (100,100,100))\n",
    "        screen.blit(textSurface1,(10,5))\n",
    "        scoreText2 = \"Episode: \" + str(episode)\n",
    "        textSurface2 = myFont.render(scoreText2, False, (100,100,100))\n",
    "        screen.blit(textSurface2,(10,30))       \n",
    "        \n",
    "    #Harder over time\n",
    "    \n",
    "        if pScore == 10 and not quicker:\n",
    "            cFrequency -= 10\n",
    "            quicker = True\n",
    "        if pScore == 11:\n",
    "            quicker = False\n",
    "            \n",
    "        if pScore == 15 and not quicker:\n",
    "            carLarge += 0.1\n",
    "            quicker = True\n",
    "        if pScore == 16:\n",
    "            quicker = False\n",
    "            \n",
    "        if pScore == 20 and not quicker:\n",
    "            pFrequency += 10\n",
    "            quicker = True\n",
    "        if pScore == 21:\n",
    "            quicker = False     \n",
    "            \n",
    "        if pScore == 25 and not quicker:\n",
    "            cFrequency -= 10\n",
    "            quicker = True\n",
    "        if pScore == 26:\n",
    "            quicker = False\n",
    "            \n",
    "        if pScore == 30 and not quicker:\n",
    "            carLarge += 0.1\n",
    "            quicker = True\n",
    "        if pScore == 31:\n",
    "            quicker = False\n",
    "            \n",
    "        if pScore == 35 and not quicker:\n",
    "            cFrequency -= 10\n",
    "            quicker = True\n",
    "        if pScore == 36:\n",
    "            quicker = False\n",
    "            \n",
    "        if pScore == 40 and not quicker:\n",
    "            carLarge += 0.1\n",
    "            quicker = True\n",
    "        if pScore == 41:\n",
    "            quicker = False\n",
    "            \n",
    "    \n",
    "    \n",
    "    #End\n",
    "        for event in pygame.event.get():\n",
    "            if event.type == pygame.QUIT:\n",
    "                play = False\n",
    "                finish = True\n",
    "                break\n",
    "            if event.type == pygame.KEYDOWN:\n",
    "                if event.key == pygame.K_SPACE:\n",
